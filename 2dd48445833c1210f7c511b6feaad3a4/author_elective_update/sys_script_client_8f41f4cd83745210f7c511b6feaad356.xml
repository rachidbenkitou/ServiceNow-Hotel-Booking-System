    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>This client script checks if the phone number field is empty. If it is, it displays an informational message indicating that the phone number cannot exceed the format of +212 followed by 9 digits.</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Phone Number Empty Check</name>
        <order/>
        <script><![CDATA[function onLoad() {
   //Type appropriate comment here, and begin script below
       if (g_form.getValue("phone_number")=== "+212") {
        g_form.showFieldMsg("phone_number", "The phone number cannot exceed +212 followed by 9 digits.", "info");
    }
}]]></script>
